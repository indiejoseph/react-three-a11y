{
  "name": "@react-three/a11y",
  "version": "2.2.4",
  "description": "👩‍🦯 Provide accessibility support to R3F such as focus indication, keyboard tab index, and screen reader support",
  "keywords": [
    "a11y",
    "accessibility",
    "three",
    "react",
    "react-three-fiber"
  ],
  "bugs": {
    "url": "https://github.com/pmndrs/react-three-a11y/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pmndrs/react-three-a11y.git"
  },
  "license": "MIT",
  "author": "Alaric Baraou",
  "main": "dist/index.js",
  "module": "dist/a11y.esm.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "analyze": "size-limit --why",
    "build": "tsdx build",
    "build-storybook": "build-storybook",
    "lint": "tsdx lint",
    "prepare": "tsdx build",
    "size": "size-limit",
    "start": "tsdx watch",
    "storybook": "start-storybook -p 6006",
    "test": "tsdx test --passWithNoTests"
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "dependencies": {
    "utility-types": "^3.10.0",
    "zustand": "^3.7.2"
  },
  "devDependencies": {
    "@babel/core": "^7.17.9",
    "@react-three/fiber": "^8.0.10",
    "@size-limit/preset-small-lib": "^7.0.8",
    "@storybook/addon-essentials": "^6.4.20",
    "@storybook/addon-info": "^5.3.21",
    "@storybook/addon-links": "^6.4.20",
    "@storybook/addons": "^6.4.20",
    "@storybook/react": "^6.4.20",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "@types/three": "^0.139.0",
    "babel-loader": "^8.2.4",
    "husky": "^7.0.4",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "react-is": "^18.0.0",
    "size-limit": "^7.0.8",
    "three": "^0.139.2",
    "tsdx": "^0.14.1",
    "tslib": "^2.3.1",
    "typescript": "^4.6.3"
  },
  "peerDependencies": {
    "@react-three/fiber": ">=8.0",
    "react": ">=18.0",
    "react-dom": ">=18.0",
    "three": ">=0.139.2"
  },
  "engines": {
    "node": ">=12.7.0"
  },
  "size-limit": [
    {
      "path": "dist/a11y.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/a11y.esm.js",
      "limit": "10 KB"
    }
  ]
}